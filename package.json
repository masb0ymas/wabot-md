{
  "name": "wabot-md",
  "version": "0.0.1",
  "description": "wabot multi device with baileys lib",
  "scripts": {
    "clean": "rimraf lib dist coverage",
    "lint": "npx eslint '*/**/*.{js,jsx,ts,tsx}' --quiet --fix",
    "prebuild": "npm-run-all clean lint",
    "build": "tsc && tsc-alias",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prerelease": "npm run build",
    "release": "standard-version",
    "release:pre": "npm run release -- --prerelease",
    "release:patch": "npm run release -- --release-as patch",
    "release:minor": "npm run release -- --release-as minor",
    "release:major": "npm run release -- --release-as major"
  },
  "keywords": [
    "wabot",
    "baileys"
  ],
  "author": "masb0ymas <n.fajri@mail.com>",
  "homepage": "https://github.com/masb0ymas/wabot-md#readme",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "sideEffects": false,
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/masb0ymas/wabot-md.git"
  },
  "bugs": {
    "url": "https://github.com/masb0ymas/wabot-md/issues"
  },
  "dependencies": {
    "@adiwajshing/keyed-db": "^0.2.4",
    "@hapi/boom": "^10.0.1",
    "@whiskeysockets/baileys": "^6.4.0",
    "mime": "^3.0.0",
    "pino": "^8.15.0",
    "pino-pretty": "^10.2.0",
    "qrcode-terminal": "^0.12.0"
  },
  "devDependencies": {
    "@types/mime": "^3.0.1",
    "@types/node": "^20.5.1",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard-with-typescript": "^38.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^16.0.2",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.2",
    "rimraf": "^5.0.1",
    "standard-version": "^9.5.0",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.7",
    "typescript": "^5.1.6"
  }
}
